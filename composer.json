{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"php": ">=5.4.0",
		"laravel/framework": "4.1.*",
		"creolab/image": "dev-master",
		"creolab/assets": "dev-master",
		"creolab/alert": "dev-master",
		"creolab/vault": "dev-master",
		"creolab/resources": "dev-master",
		"mobiledetect/mobiledetectlib": "dev-master",
		"itsgoingd/clockwork": "dev-master",
		"vlucas/phpdotenv": "1.*"
	},
	"autoload": {
		"classmap": [
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-4": {
			"App\\Commands\\": "app/commands",
			"App\\Controllers\\": "app/controllers",
			"App\\Facades\\": "app/facades",
			"App\\Models\\": "app/models",
			"App\\Providers\\": "app/providers",
			"App\\Repositories\\": "app/repositories",
			"App\\Resources\\": "app/resources",
			"App\\Services\\": "app/services",
			"App\\Validation\\": "app/validation"
		},
		"files": [
			"app/helpers.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "source"
	},
	"minimum-stability": "dev"
}
